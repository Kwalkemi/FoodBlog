<div class="pouplar_post">
    <div class="card">
        <div class="card-header text-aligned-center">
            Popular Post
        </div>
        <ul class="list-group list-group-flush">
            @foreach (var item in ViewBag.listPopularPost)
            {
                <li class="list-group-item text-aligned-center">@item</li>
            }
        </ul>
    </div>
</div>
