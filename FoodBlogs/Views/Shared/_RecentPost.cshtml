<div class="recentpost">
    <div class="card">
        <div class="card-header text-aligned-center">
            Recent Post
        </div>
        <ul class="list-group list-group-flush">
            @foreach (var item in ViewBag.listRecentPost)
            {
                <li class="list-group-item text-aligned-center">@item</li>
            }
        </ul>
    </div>
</div>

